{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5163"
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:7225;http://localhost:5163"
    },
    "SaaS+Agent": {
      "commandName": "Executable",
      "executablePath": "cmd",
      "commandLineArgs": "/c scripts\\run-all.cmd",
      "workingDirectory": "."
    },
    "Stop SaaS+Agent": {
      "commandName": "Executable",
      "executablePath": "cmd",
      "commandLineArgs": "/c scripts\\stop-all.cmd",
      "workingDirectory": "."
    }
  }
}
